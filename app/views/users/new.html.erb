<h1>Sign up</h1>
<%if (@error_messages != nil)%>
<ul>
<%@error_messages.each do |e|%>
    <li><%= e %></li>
<%end%>
</ul>
<%end%>

<%=form_with(model:@user) do |f|%>
<ul>
<label> Name </label>
<%=f.text_field :name%>
<br>
<label> Email: </label>
<%=f.text_field :email_address%>
<br>
<label> Username:</label>
<%=f.text_field :user_name%>
<br>
<label> Password: </label>
<%=f.password_field :password%>
<br>
<%=f.submit%>
</ul>
<%end%>

   
  
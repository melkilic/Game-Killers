<%= form_with({ method: 'POST', url: '/users/login' }) do  | form | %>
    <div>

        <label>Username</label>
        <%= form.text_field(:user_name) %>
        <br>
    </div>
    <div>
        <label>Password</label>
        <%= form.password_field(:password) %>
        <br>
  
    </div>
    <%= form.submit('Login') %>
<% end %>